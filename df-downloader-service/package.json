{
  "name": "df-downloader-service",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "tsc --watch",
    "build": "tsc",
    "dev": "env-cmd -f dev.env --silent nodemon --watch dist dist/index.js",
    "start": "node dist/index.js",
    "get-ui": "rimraf public && cp -r ../df-downloader-ui/build public",
    "build-get-ui": "cd ../df-downloader-ui && npm run build && cd ../df-downloader-service && npm run get-ui",
    "add-common": "yalc add df-downloader-common",
    "full-build": "npm i && npm run add-common && npm run build",
    "remove:config": "rimraf config/users.yaml && rimraf config/config.yaml",
    "remove:data": "rimraf db/db.json",
    "remove": "rimraf .yalc && rimraf yalc.lock && rimraf dist && rimraf public; rimraf node_modules",
    "remove:all": "npm run remove:config && npm run remove:data && npm run remove"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@deepgram/sdk": "^1.18.1",
    "bcrypt": "^5.1.0",
    "better-queue": "3.8.10",
    "chalk": "^5.2.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cryptr": "^6.2.0",
    "css-select": "4.2.1",
    "df-downloader-common": "file:.yalc/df-downloader-common",
    "express": "^4.18.1",
    "ffmpeg-static": "5.0.0",
    "got": "^12.3.1",
    "htmlparser2": "7.2.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "lowdb": "^3.0.0",
    "mv": "2.1.1",
    "node-forge": "^1.3.1",
    "pretty-bytes": "6.0.0",
    "string-argv": "^0.3.1",
    "utimes": "5.2.1",
    "yaml": "^2.2.1",
    "zod-validation-error": "^1.2.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/better-queue": "3.8.3",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.13",
    "@types/ffmpeg-static": "3.0.1",
    "@types/got": "9.6.12",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.184",
    "@types/mv": "2.1.2",
    "@types/node": "^18.15.11",
    "@types/node-forge": "^1.3.2",
    "env-cmd": "10.1.0",
    "nodemon": "^2.0.19",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.4",
    "yalc": "^1.0.0-pre.53"
  }
}
